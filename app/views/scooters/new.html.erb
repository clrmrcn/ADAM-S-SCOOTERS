<div class="container">
  <h1>Add a new scooter</h1>
  <%= simple_form_for(@scooter) do |f| %>
    <%= f.input :model %>
    <%= f.input :description %>
    <%= f.input :photo, as: :file %>
    <%= f.input :price %>
    <%= f.input :available %>
    <%= f.input :address, label: "address", input_html: { id: "scooter_address"} %>

    <%= f.submit %>
  <% end %>
</div>
